{
  "name": "zendesk-intercom-app",
  "author": {
    "name": "Mark Wainwright",
    "email": "mark@unbounce.com"
  },
  "frameworkVersion": "1.0",
  "location": [
    "ticket_sidebar",
    "new_ticket_sidebar",
    "user_sidebar"
  ],
  "defaultLocale": "en",
  "private": true,
  "version": "0.7.1",
  "domainWhitelist": ["api.intercom.io"],
  "parameters": [
    {
      "name": "intercomAppID",
      "type": "text",
      "required": true
    },
    {
      "name": "intercomAPIKey",
      "type": "password",
      "required": true,
      "secure": true
    }
  ]
}
